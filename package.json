{
  "name": "material2-start",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve --port 4256",
    "restart": "rm -rf node_modules/angular2gridster/ && npm install && npm start",
    "serve": "npm run build:aot && npm run angular-http-server",
    "serve:aot": "npm run build-themes && ng serve --prod --aot",
    "build": "ng build",
    "build:aot": "ng build --prod --aot",
    "lint": "ng lint",
    "workshop": "ts-node ./workshop-cli",
    "build-themes": "bash ./tools/build-themes.sh",
    "bundle-report": "ng build --prod --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "angular-http-server": "angular-http-server --path ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.6",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.6",
    "@angular/compiler": "6.1.6",
    "@angular/core": "6.1.6",
    "@angular/flex-layout": "6.0.0-beta.17",
    "@angular/forms": "6.1.6",
    "@angular/material": "6.4.7",
    "@angular/platform-browser": "6.1.6",
    "@angular/platform-browser-dynamic": "6.1.6",
    "@angular/platform-server": "6.1.6",
    "@angular/router": "6.1.6",
    "@angular/service-worker": "6.1.6",
    "angular2gridster": "6.0.2",
    "chartist": "0.11.0",
    "core-js": "2.5.7",
    "hammerjs": "2.0.8",
    "ng-chartist": "1.1.1",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "6.1.6",
    "@types/chartist": "0.9.42",
    "@types/jasmine": "2.5.46",
    "@types/node": "7.0.12",
    "angular-http-server": "1.5.0",
    "codelyzer": "4.0.1",
    "node-sass": "4.6.0",
    "simple-git": "1.69.0",
    "simple-terminal-menu": "1.1.3",
    "ts-node": "3.0.4",
    "tslint": "5.3.2",
    "typescript": "2.7.2",
    "webpack-bundle-analyzer": "2.9.0",
    "@angular-devkit/build-angular": "0.6.8"
  }
}
